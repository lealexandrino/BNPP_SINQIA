<div>
    <h4>Produto Cosif</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.CodClassificacao)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CodClassificacao)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.StaStatus)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.StaStatus)
        </dd>
        <dt>
